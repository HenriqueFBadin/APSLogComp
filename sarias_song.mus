music_base { instrument "guitar" 
tempo_base 120 }

var d note = note D5 duration 2
var fs note = note F#5 duration 2
var e note = note E5 duration 2
var g note = note G5 duration 2

repeat 2 times 
{
    play_note d
    play_note fs
    play_note d
    pause duration 1
}

repeat 2 times 
{
    play_note e
    play_note g
    play_note e
    pause duration 1
}
